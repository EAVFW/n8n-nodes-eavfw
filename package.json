{
  "name": "n8n-nodes-eavfw",
  "version": "1.0.0",
  "description": "EAVFW nodes for n8n",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Poul Kjeldager",
    "email": "poul@kjeldager.com"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "docker:build": "docker build --build-arg N8N_VERSION=1.78.1 --tag=registry.kjeldager.io/n8n/customizedn8n .",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.js"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/EAVFWOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/EAVFW/EAVFW.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^18.19.14",
    "@types/request-promise-native": "~1.0.21",
    "@typescript-eslint/parser": "~7.0.1",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-core": "^1.33.1",
    "n8n-workflow": "^1.33.1",
    "prettier": "^3.2.5",
    "typescript": "~5.3.3"
  }
}